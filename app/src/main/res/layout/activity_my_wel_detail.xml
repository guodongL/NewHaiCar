<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_my_wel_detail"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:tag="skin:background2:background"
    tools:context="com.example.administrator.myhaicar.main.mine.Event.Acticity.MyWelDetailActivity">
    <android.support.v7.widget.Toolbar
        android:id="@+id/toolBar_myAccount"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fitsSystemWindows="true"
        android:background="@drawable/appbar_day"
        android:tag="skin:appbar:background">
        <ImageView
            android:id="@+id/toolBar_back"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_marginLeft="17dp"
            android:padding="5dp"
            android:cropToPadding="true"
            android:src="@drawable/back"
            android:layout_alignBottom="@+id/linearLayout3"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="17dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="详细信息"
            android:textSize="18sp"
            android:textColor="#fff"
            android:layout_gravity="center"
            />
        <ImageView
            android:id="@+id/image_myWelfare"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:padding="5dp"
            android:cropToPadding="true"
            android:src="@drawable/share"
            android:layout_marginRight="10dp"/>
    </android.support.v7.widget.Toolbar>
    <WebView
        android:id="@+id/webView_welfareDetail"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="75dp"></WebView>
</RelativeLayout>
